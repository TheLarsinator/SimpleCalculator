<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="thelarsinator.simplecalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".simplemath.SimpleMath" />
        <activity android:name=".polynomials.SolvePolynomials" />
        <activity android:name=".angles.AngleMath" />
        <activity android:name=".complex.ComplexMath" />
        <activity android:name=".time.TimeMath" />
    </application>

</manifest>